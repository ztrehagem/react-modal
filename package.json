{
  "name": "@ztrehagem/react-modal",
  "version": "2.0.0-alpha.5",
  "description": "Stacking-managed styleless modal library for React.",
  "keywords": [
    "modal",
    "react"
  ],
  "author": {
    "email": "shigeki.suwa@ztrehagem.dev",
    "name": "Shigeki Suwa",
    "url": "https://ztrehagem.dev"
  },
  "license": "MIT",
  "homepage": "https://react-modal.lib.ztrehagem.dev",
  "bugs": "https://github.com/ztrehagem/react-modal/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ztrehagem/react-modal.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/react-modal.cjs.js",
  "module": "dist/react-modal.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-modal.es.js",
      "require": "./dist/react-modal.cjs.js"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/types/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "npm run build:lib",
    "dev": "vite",
    "build:lib": "vite build --mode=lib && tsc -p tsconfig.dts.json",
    "build:site": "vite build",
    "preview": "vite preview",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ignore-path=.gitignore .",
    "lint:prettier": "prettier --ignore-path=.gitignore --check .",
    "fix": "run-s fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-transition-group": "^4.4.4",
    "react": "^18.0.0",
    "react-transition-group": "^4.4.2",
    "recoil": "^0.7.2"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.17",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vanilla-extract/css": "^1.7.0",
    "@vanilla-extract/vite-plugin": "^3.1.4",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-transition-group": "^4.4.2",
    "recoil": "^0.7.2",
    "typescript": "^4.5.5",
    "vite": "^2.8.1"
  }
}
